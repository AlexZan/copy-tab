{
  "name": "copy-tab",
  "displayName": "Copy Tab",
  "description": "CopyTab is a VS Code extension that enhances your workflow by copying the content of all open tabs into your clipboard in a single command. It's an ideal tool for quickly sharing code, transferring work between files, or pasting your code directly into GPT chat for AI-powered assistance. Streamline your coding process with CopyTab.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "activationEvents": [
    "onCommand:extension.copyAllOpenFiles"
  ],
  "contributes": {
    "commands": [
      {
        "command": "extension.activateCopyAllOpenFiles",
        "title": "Activate Copy All Open Files"
      },
      {
        "command": "extension.copyAllOpenFiles",
        "title": "Copy All Open Files"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.80.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "copy-paste": "^1.5.3"
  }
}